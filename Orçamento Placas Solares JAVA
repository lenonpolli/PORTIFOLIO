	public Ver() {
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setBounds(100, 100, 818, 557);
		contentPane = new JPanel();
		contentPane.addMouseListener(new MouseAdapter() {
			@Override
			public void mouseEntered(MouseEvent e) {
			}
		});
		contentPane.setBackground(new Color(192, 192, 192));

		setContentPane(contentPane);
		contentPane.setLayout(null);
		
		JButton btnNewButton = new JButton("CALCULAR");
		btnNewButton.setBackground(new Color(0, 0, 0));
		btnNewButton.setForeground(new Color(255, 0, 0));
		btnNewButton.setFont(new Font("Times New Roman", Font.BOLD, 18));
		btnNewButton.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
				
				
				
				
				double janeiro,fevereiro,marco,abril,maio,junho,julho,agosto,setembro,outubro,novembro,dezembro;
				
				double resultadomedia, mediaa;
				
				
				
				
				janeiro = Double.parseDouble(txtJaneiro.getText());
				fevereiro = Double.parseDouble(txtFevereiro.getText());
				marco = Double.parseDouble(txtMarco.getText());
				abril = Double.parseDouble(txtAbril.getText());
				maio = Double.parseDouble(txtMaio.getText());
				junho = Double.parseDouble(txtJunho.getText());
				julho = Double.parseDouble(txtJulho.getText());
				agosto = Double.parseDouble(txtAgosto.getText());
				setembro = Double.parseDouble(txtSetembro.getText());
				outubro = Double.parseDouble(txtOutubro.getText());
				novembro = Double.parseDouble(txtNovembro.getText());
				dezembro = Double.parseDouble(txtDezembro.getText());
			
				resultadomedia = janeiro + fevereiro + marco + abril + maio + junho + julho + agosto + setembro + outubro + novembro + dezembro;
				mediaa = resultadomedia /12;
				
				
				double consumoDiario;
				
				consumoDiario = mediaa/30.4;
				
				double potencia;
				
				potencia = (consumoDiario/0.76)/4.78;
				
				double quantidadePlacas,placas;
				
				placas = Double.parseDouble(textPlacas.getText());
				
				quantidadePlacas = (potencia*1000)/placas;
			
				double area;
				
				area = quantidadePlacas*2.16594;
				
				if (placas <= 500) {
					
					txtArea.setText(""+Math.ceil(area));
				
				}else {
					area = quantidadePlacas*2.584386;
					txtArea.setText(""+Math.ceil(area));
				}
				
				
				double geracaoJaneiro,geracaoFevereiro,geracaoMarco,geracaoAbril,geracaoMaio,geracaoJunho,geracaoJulho,geracaoAgosto,geracaoSetembro,geracaoOutubro,geracaoNovembro,geracaoDezembro;
				
				
				geracaoJaneiro = potencia*6.43*30.4*0.76;
				geracaoFevereiro = potencia*5.91*30.4*0.76;
				geracaoMarco = potencia*5.35*30.4*0.76;
				geracaoAbril = potencia*4.19*30.4*0.76;
				geracaoMaio = potencia*3.27*30.4*0.76;
				geracaoJunho = potencia*2.79*30.4*0.76;
				geracaoJulho = potencia*3.02*30.4*0.76;
				geracaoAgosto = potencia*3.98*30.4*0.76;
				geracaoSetembro = potencia*4.28*30.4*0.76;
				geracaoOutubro = potencia*5.29*30.4*0.76;
				geracaoNovembro = potencia*6.17*30.4*0.76;
				geracaoDezembro = potencia*6.63*30.4*0.76;
				
				
				
				textGeracao.setText("Mês de Janeiro: "+Math.ceil(geracaoJaneiro));
				txtGeracaoFevereiro.setText("Mês de Fevereiro: "+Math.ceil(geracaoFevereiro));
				txtGeracaoMarco.setText("Mês de Março: "+Math.ceil(geracaoMarco));
				txtGeracaoAbril.setText("Mês de Abril: "+Math.ceil(geracaoAbril));
				txtGeracaoMaio.setText("Mês de Maio: "+Math.ceil(geracaoMaio));
				txtGeracaoJunho.setText("Mês de Junho: "+Math.ceil(geracaoJunho));
				txtGeracaoJulho.setText("Mês de Julho: "+Math.ceil(geracaoJulho));
				txtGeracaoAgosto.setText("Mês de Agosto: "+Math.ceil(geracaoAgosto));
				txtGeracaoSetembro.setText("Mês de Setembro: "+Math.ceil(geracaoSetembro));
				txtGeracaoOutubro.setText("Mês de Outubro: "+Math.ceil(geracaoOutubro));
				txtGeracaoNovembro.setText("Mês de Novembro: "+Math.ceil(geracaoNovembro));
				txtGeracaoDezembro.setText("Mês de Dezembro: "+Math.ceil(geracaoDezembro));
				
				textResultado.setText(""+Math.ceil(mediaa));
				textConsumo.setText(""+Math.ceil(consumoDiario));
				textPotencia.setText(""+Math.ceil(potencia));
				txtQuantidade.setText(""+Math.ceil(quantidadePlacas));
				
				
				
			}
		});
		btnNewButton.setBounds(7, 403, 145, 42);
